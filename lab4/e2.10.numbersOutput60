// vim: set ft=java sw=4 ts=4 :

for (int i = 1; i <= 6; i++) {
    for (int j = 1; j <= 9; j++) {
        System.out.print(" ");
    }
    System.out.print("|");
}
System.out.print("\n");

for (int i = 1; i <= 6; i++) {
    for (int j = 1; j <= 9; j++) {
        System.out.print(j);
    }
    System.out.print(0);
}
